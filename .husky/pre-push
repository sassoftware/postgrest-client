git diff HEAD --quiet || {
    if [ $? -eq '1' ]; then
        echo -e "You have uncommitted changes.\nEither commit or stash them before pushing again.";
        exit 1;
    fi

};

npm run lint;
npm run format:check;
npm run circular-deps;
npm run test:ci -- --coverage;
